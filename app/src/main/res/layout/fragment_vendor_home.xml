<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.VendorHomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="95dp"
            android:layout_height="28dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="16dp"
            android:text="Name"
            app:layout_constraintEnd_toStartOf="@+id/nameEdit"
            app:layout_constraintHorizontal_bias="0.549"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="95dp"
            android:layout_height="28dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:text="Address"
            app:layout_constraintEnd_toStartOf="@+id/addressEdit"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="105dp"
            android:layout_height="42dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:text="Phone Number"
            app:layout_constraintEnd_toStartOf="@+id/priceEdit"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="95dp"
            android:layout_height="28dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:text="Price"
            app:layout_constraintEnd_toStartOf="@+id/priceEdit"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <Spinner
            android:id="@+id/spinnerChargerType"
            android:layout_width="287dp"
            android:layout_height="48dp"
            android:layout_marginTop="12dp"
            android:entries="@array/items_class"
            android:text="Charger Type"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.588"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/priceEdit" />

        <Button
            android:id="@+id/buttonUpdate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Update details "
            android:layout_marginTop="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonDeRegister"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/buttonDeRegister"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="DEREGISTER"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextEndTime" />

        <EditText
            android:id="@+id/nameEdit"
            android:layout_width="194dp"
            android:layout_height="48dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="40dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <EditText
            android:id="@+id/addressEdit"
            android:layout_width="194dp"
            android:layout_height="48dp"
            android:layout_marginEnd="40dp"
            android:ems="10"
            android:inputType="text"
            android:text="bits hyderabad"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/nameEdit" />

        <EditText
            android:id="@+id/priceEdit"
            android:layout_width="194dp"
            android:layout_height="48dp"
            android:layout_marginEnd="40dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="111"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/phoneEdit" />

        <EditText
            android:id="@+id/phoneEdit"
            android:layout_width="194dp"
            android:layout_height="48dp"
            android:layout_marginEnd="40dp"
            android:ems="10"
            android:inputType="number"
            android:text="Phone no."
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/addressEdit"  />

        <Switch
            android:id="@+id/switch3"
            android:layout_width="134dp"
            android:layout_height="57dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="132dp"
            android:text="Station Not Available"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spinnerChargerType" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="108dp"
            android:src="@drawable/screenshot_2022_11_28_190018"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageButton
            android:id="@+id/pick_time_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="492dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/ic_baseline_date_range_24"
            android:text="Start Time"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.165"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextStartTime"
            android:layout_width="146dp"
            android:layout_height="46dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="100dp"
            android:ems="10"
            android:focusable="false"
            android:clickable="false"
            android:hint="Start Time"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch3" />

        <ImageButton
            android:id="@+id/end_time_button"
            android:layout_width="53dp"
            android:layout_height="45dp"
            android:layout_marginTop="552dp"
            android:src="@drawable/ic_baseline_date_range_24"
            android:text="End Time"
            android:layout_marginLeft="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.153"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextEndTime"
            android:layout_width="135dp"
            android:layout_height="51dp"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="100dp"
            android:ems="10"
            android:focusable="false"
            android:clickable="false"
            android:hint="End Time"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch3" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>